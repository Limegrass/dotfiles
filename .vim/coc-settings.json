{
    "suggest.enablePreselect": false,
    "suggest.snippetIndicator": "+",
    "suggest.enablePreview": true,
    "codeLens.enable": true,
    "codeLens.separator": "|->",
    "languageserver": {
        "omnisharp": {
            "command": "OmniSharp",
            "args": ["-lsp"],
            "rootPatterns": [".sln"],
            "filetypes": [ "cs" ]
        },
        "mssql": {
            "command": "MicrosoftSqlToolsServiceLayer.exe",
            "filetypes": [ "sql" ]
        },
        "kotlin": {
            "command": "kotlin-language-server.bat",
            "filetypes": [ "kotlin" ]
        },
        "golang": {
            "command": "gopls",
            "rootPatterns": ["go.mod"],
            "filetypes": ["go"]
        }
    },
    "python.jediEnabled": false,
    "git.enableGutters": true,
    "git.addGBlameToVirtualText": true,
    "git.virtualTextPrefix": "    |->"
}
